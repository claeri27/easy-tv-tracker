{
  "name": "easy-tv-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "nodemon": "^1.19.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "react": "^16.8.6",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "sequelize": "^5.9.4",
    "styled-components": "^4.3.2",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "server": "nodemon ./src/server/server.js",
    "db:start": "cd ./src/server && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:undo": "cd ./src/server && npx sequelize-cli db:seed:undo:all && npx sequelize-cli db:migrate:undo:all",
    "db:undo:one": "cd ./src/server && sequelize db:migrate:undo",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.3"
  }
}
